%div.page-header.text-center#center-header
  %h1 Fitness Centers Nearby
.container
  .row
    %div#map-canvas.col-md-6.col-md-offset-3
      %iframe{src: "https://www.google.com/maps/embed/v1/search?key=#{@api_key}&q=fitness+centers+near+#{@location[:zip_code]}&center=#{@location[:latitude]},#{@location[:longitude]}&zoom=10", style: {width: "100%"}}
