%div.page-header.text-center#center-header
  %h1 All Users:
%div.container
  %div.row.text-center
    %div.col-md-6.col-md-offset-3
      %table.table.table-bordered
        %thead
          %tr
          %th First Name
          %th Last Name
          %th Email
          %th Status
        %tbody   
          -@users.each do |user|
            %tr
            %td=user.first_name
            %td=user.last_name
            %td=user.email
            %td
              =user.role
              .path#edit=link_to "Edit", edit_admin_user_path(user)
              .path#delete=link_to "Delete", admin_user_path(user), method: "delete"
        
