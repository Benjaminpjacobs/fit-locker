%div.page-header.text-center#center-header
  %h3.text-center Cardio
%div.container
  %div.row.text-center
    %div.col-md-6.col-md-offset-3
      %div.list-group
        - @cardio_activities.each do |activity|
          %a{:href => "https://www.google.com/search?q=#{search_string(activity.name)}+demonstation", :target => '_blank'}
            %div.list-group-item#workouts
              %h3.list-group-item-heading
                =activity.name.capitalize
    

